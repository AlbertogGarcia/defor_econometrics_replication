@article{bollinger,
	title = {Visibility and {Peer} {In} uence in {Durable} {Good} {Adoption}},
	abstract = {The underlying channels through which peer in uence operates in durable good adoption can a ect the ability of marketers to leverage them. In this paper, we assess whether the visibility of peers‚Äô adoption decisions leads to greater peer in uence. The context we study is residential rooftop solar panels. We exploit the plausibly exogenous location and orientation of peers‚Äô rooftop solar panels relative to proximate roadways and visual obstructions, such as vegetation, in order to determine whether geographically proximate peer installations increase a household‚Äôs probability of solar adoption more if they are visible from the road. We nd that the total angle of visibility of peer installations on the same street positively a ects solar adoption decisions at distances of at least 500 meters. In contrast, we only nd a positive e ect of non-visible solar arrays within 100m, which may be due to causal peer in uence via other channels such as word-of-mouth, or very localized unobservable e ects. The e ect of peer visibility is moderated by the economic value the peers receive from installing solar, providing suggestive evidence of social learning through visual information.},
	language = {en},
	author = {Bollinger, Bryan and Gillingham, Kenneth and Sexton, Steven and Kirkpatrick, A Justin},
	pages = {65},
	year = {2021},
