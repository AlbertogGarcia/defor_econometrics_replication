@article{panlasigui2018,
	title = {Impacts of certification, uncertified concessions, and protected areas on forest loss in {Cameroon}, 2000 to 2013},
	abstract = {Deforestation and forest fragmentation are leading drivers of biodiversity loss. Protected areas have been the leading conservation policy response, yet their scale and scope remain inadequate to meet biodiversity conservation targets. Managed forest concessions increasingly have been recognized as a complement to protected areas in meeting conservation targets. Similarly, programs for voluntary third-party certiÔ¨Åcation of concession management aim to create incentives for logging companies to manage forests more sustainably. Rigorous evidence on the impacts from large-scale certiÔ¨Åcation programs is thereby critical, yet detailed Ô¨Åeld observations are limited, temporally and spatially. Remotely-sensed data, in contrast, can provide repeated observations over time and at a Ô¨Åne spatial scale, albeit with less detail. Using the Global Forest Change dataset, we examine annual forest loss in Cameroon during 2000‚Äì2013 to assess the impact of Forest Stewardship Council certiÔ¨Åcation, as well as uncertiÔ¨Åed logging concessions and national parks. We use panel regressions that control for the eÔ¨Äects of unobserved factors that vary across space or time. We Ô¨Ånd low forest loss inside the boundaries of each management intervention, with {\textless} 1\% lost over the study period. Yet those low levels of loss appear to be inÔ¨Çuenced more by a site's proximity to drivers of deforestation, such as distances to population centers or roads, than by national parks, uncertiÔ¨Åed concessions, or certiÔ¨Åcation. The exception is that if a site faces high deforestation pressure, uncertiÔ¨Åed logging concessions appear to reduce forest loss. This may reÔ¨Çect private companies' incentives to protect rights to forest use. Such an inÔ¨Çuence of private logging companies could provide a foundation for future impacts from certiÔ¨Åcation upon rates of forest loss, at least within areas that are facing elevated deforestation pressures.},
	language = {en},
	journal = {Biological Conservation},
	author = {Panlasigui, Stephanie},
	year = {2018},
	pages = {7},
	file = {Panlasigui - 2018 - Impacts of certification, uncertified concessions,.pdf:C\:\\Users\\garci\\Zotero\\storage\\JGUK259P\\Panlasigui - 2018 - Impacts of certification, uncertified concessions,.pdf:application/pdf}
}


@article{gardner2021,
	title = {Two-stage diÔ¨Äerences in diÔ¨Äerences},
	abstract = {A recent literature has shown that when adoption of a treatment is staggered and average treatment eÔ¨Äects vary across groups and over time, diÔ¨Äerence-in-diÔ¨Äerences regression does not identify an easily interpretable measure of the typical eÔ¨Äect of the treatment. In this paper, I extend this literature in two ways. First, I provide some simple underlying intuition for why diÔ¨Äerence-in-diÔ¨Äerences regression does not identify a group√óperiod average treatment eÔ¨Äect. Second, I propose an alternative twostage estimation framework, motivated by this intuition. In this framework, group and period eÔ¨Äects are identiÔ¨Åed in a Ô¨Årst stage from the sample of untreated observations, and average treatment eÔ¨Äects are identiÔ¨Åed in a second stage by comparing treated and untreated outcomes, after removing these group and period eÔ¨Äects. The two-stage approach is robust to treatment-eÔ¨Äect heterogeneity under staggered adoption, and can be used to identify a host of diÔ¨Äerent average treatment eÔ¨Äect measures. It is also simple, intuitive, and easy to implement. I establish the theoretical properties of the two-stage approach and demonstrate its eÔ¨Äectiveness and applicability using Monte-Carlo evidence and an example from the literature.},
	language = {en},
	journal = {Working Paper},
	author = {Gardner, John},
	month = apr,
	year = {2021},
	pages = {34},
	file = {Gardner - Two-stage diÔ¨Äerences in diÔ¨Äerences.pdf:C\:\\Users\\garci\\Zotero\\storage\\463LMUH5\\Gardner - Two-stage diÔ¨Äerences in diÔ¨Äerences.pdf:application/pdf},
}

